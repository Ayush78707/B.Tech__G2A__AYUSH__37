#include <stdio.h>
int main()
  {
    printf("Enter the Character\n");
    char ch;
    scanf("%c",&ch);
    if((ch>=97 && ch<=122) ||(ch>=65 && ch<=90)){
        printf("%c is alphabet", &ch);
    }
    else if(ch>=48 && ch<=57)
    {
    printf("%c is digit\n",&ch);
    }
    else
    {
    printf("%c is a special character\n" , ch);
    }
    return 0;
}
